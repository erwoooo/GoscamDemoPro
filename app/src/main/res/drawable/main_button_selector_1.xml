<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape
               android:shape="rectangle">
            <stroke android:color="@color/_8BE2C0" android:width="@dimen/w_2px"/>
            <corners android:radius="@dimen/w_10px"/>
        </shape>
    </item>
    <item>
        <shape
               android:shape="rectangle">
            <stroke android:color="@color/_3CCF9B" android:width="@dimen/w_2px"/>
            <corners android:radius="@dimen/w_10px"/>
        </shape>
    </item>
</selector>
